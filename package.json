{
  "name": "user-service",
  "version": "0.1.0",
  "description": "User services",
  "main": "index.js",
  "scripts": {
    "start": "tsc && node ./dist/index.js",
    "build": "tsc",
    "serve": "node ./dist/index.js",
    "test": "npm run tslint && npm run run_tests",
    "run_tests": "npm run test:unit",
    "test:unit": "cross-env NODE_ENV=test mocha --recursive -r ts-node/register src/**/*.unit.spec.ts --exit",
    "test_mocha": "cross-env NODE_ENV=test mocha --recursive -r ts-node/register src/**/*.spec.ts --exit",
    "test:integration": "cross-env NODE_ENV=test mocha --recursive -r ts-node/register \"src/**/*.integration.spec.ts\" --exit",
    "test:coverage": "cross-env NODE_ENV=test nyc npm test",
    "coverage-test": "cross-env NODE_ENV=test nyc check-coverage --lines 95 --functions 95 --branches 95",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "release": "standard-version",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "author": "Yonatan Tal",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.4",
    "@types/chai-as-promised": "^7.1.2",
    "@types/elasticsearch": "^5.0.36",
    "@types/google-protobuf": "^3.2.7",
    "@types/mocha": "^7.0.2",
    "@types/mongoose": "^5.2.22",
    "@types/node": "^10.17.17",
    "@types/request-promise-native": "^1.0.15",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^5.2.0",
    "grpc_tools_node_protoc_ts": "^3.0.0",
    "mocha": "^5.2.0",
    "nyc": "^13.3.0",
    "standard-version": "^4.4.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "elastic-apm-node": "2.11.6",
    "grpc": "^1.23.3",
    "grpc-ts-health-check": "^1.0.14",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "snyk": "^1.316.1",
    "winston": "^3.2.1",
    "winston-elasticsearch": "^0.7.13"
  },
  "snyk": true
}
